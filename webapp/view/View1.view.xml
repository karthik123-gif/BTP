<Input id="passwordInput" width="200px" type="Password" showClearIcon="true" valueState="Error" valueStateText="Password required"/>
</HBox>
<FlexBox id="_IDGenFBox1" justifyContent="Center">
<Button id="_IDGenButton2" text="login" type="Accept" class="sapUiSmallMargin" press="onLoginPress"/>
