         / -->
<FlexBox id="_IDGenFlexBox1" justifyContent="Center" alignItems="Center">
<VBox id="_IDGenVBox1" alignItems="Center" width="100%">
<Image id="hj" src="https://pngteam.com/images/staff-only-symbol-png-300x300_e598fa52_transparent_202562.png.png" width="200px"/>
<!-- Button id="_IDGenButton1" icon="sap-icon://customer"/ -->
<!-- Text id="_IDGenText1" text="login"  class="HeaderLabel sapUiSmallMargin" ></Text -->
<HBox id="_IDGenHBox1">
<Label id="_IDGenLabel1" text="User Name" width="100px" required="true"/>
<Input id="usernameInput" width="200px" showClearIcon="true" valueState="Error" valueStateText="Name required"/>
</HBox>
